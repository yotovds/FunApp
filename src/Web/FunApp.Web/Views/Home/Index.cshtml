@model FunApp.Web.Models.Home.IndexViewModel

@{
    ViewData["Title"] = "Home Page";
}

<div class="row">
    @foreach (var joke in Model.Jokes)
    {
    <div class="col-md-4">
        <div class="panel panel-default clearfix">
            <div class="panel-body">
                @joke.Content
            </div>
            <div class="panel-footer">@joke.CategoryName</div>
        </div>
    </div>
    }
</div>
